<Window x:Class="ResumeBuilderUI.ContactEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResumeBuilderUI"
        mc:Ignorable="d"
        Title="Edit Contact" Height="250" Width="200">
    <StackPanel>
        <Label Name="lblTypeOfContact" Content="Type of Contact"/>
        <TextBox Name="txtTypeOfContact" Text=""/>
        <Label Name="lblContactInfo" Content="Contact Info"/>
        <TextBox Name="txtContactInfo" Text=""/>
        <Button Name="btnAccept" Click="btnAccept_Click" Content="Accept" Width="100" Margin="0,100,0,0"/>

    </StackPanel>
</Window>
