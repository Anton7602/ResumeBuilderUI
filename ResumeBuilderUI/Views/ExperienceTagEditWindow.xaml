<Window x:Class="ResumeBuilderUI.ExperienceTagEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResumeBuilderUI"
        mc:Ignorable="d"
        Title="Experience Tag Edit" Height="150" Width="150">
    <StackPanel>
        <Label Name="lblExperienceTag" Content="Experience Tag"/>
        <TextBox Name="txtExperienceTag" Text="" Margin="5"/>
        <Button Name="btnExperienceTag" Content="Accept" Width="100" Margin="0, 25, 0, 0" Click="btnAccept_Click"/>
    </StackPanel>
</Window>
