<Application x:Class="ResumeBuilderUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ResumeBuilderUI"
             xmlns:views="clr-namespace:ResumeBuilderUI.Views" 
             xmlns:system="clr-namespace:System;assembly=netstandard"
             xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resourses/Localization/lang.xaml"/>
                <ResourceDictionary Source="Resourses/Styles/userControlsStyles.xaml"/>
                <ResourceDictionary Source="Resourses/Styles/viewsStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
